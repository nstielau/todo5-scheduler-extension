{
  "name": "Todo5 Scheduler",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Schedule your todos on your calendar.",
  "homepage_url": "http://github.com/nstielau/todo5",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "service_worker": "src/bg/background.js",
    "type": "module"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "oauth2": {
    "client_id": "491406573957-qf0qt6bq7e3hs22v6fk73dsitv3a45g7.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  }
}